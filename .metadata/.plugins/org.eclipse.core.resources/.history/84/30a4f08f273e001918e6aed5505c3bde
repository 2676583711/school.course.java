package school.course;

/*
 *关于 java 的对象的学习
 */
class Person {
	String name;
	int age;

	public void speak() {
		System.out.println("我叫"+name+",今年"+age+"岁了");
	}
}

public class AboutClass {

	class Person {
		String name;
		int age;

		public void speak() {
			System.out.println("我叫"+name+",今年"+age+"岁了");
		}
	}
	
	public static void main(String[] args) {
		Person p=new Person();
		p.name="张三";
		p.age=250;
		p.speak();
	}
}
