/*
 * สตั้ static ด๚ย๋ฟ้
 */
public class Person {

	static {
		System.out.println("person.static code");
	}

	public static void main(String[] args) {

		Test t = new Test();  //person.static code
		test.static code

		Test t2 = new Test();

	}

}

///***********************************/**************//*/**/*/*/*/*/*/

class Test {
	static {

		System.out.println("test.static code");
	}
}